// Write a program to calculate sum of 5 subjects & find the percentage.
// Subject marks entered by user.
fun main()
{
   var sub1 = 66// subject 1
   var sub2 = 72// subject 2
   var sub3 = 68// subject 3
   var sub4 = 81// subject 4
   var sub5 = 77// subject 5

   var total = (sub1 + sub2 + sub3 + sub4 + sub5)

   var percentage:Double= (total/5.00)

   println(percentage)
}
